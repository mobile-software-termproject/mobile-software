<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DisplayDietEnter">

    <ImageView
        android:id="@+id/imageSanglok1"
        android:layout_width="180dp"
        android:layout_height="101dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/imageSanglok2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/sanglok1" />

    <ImageView
        android:id="@+id/imageSanglok3"
        android:layout_width="197dp"
        android:layout_height="111dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="29dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/imageDomitory"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/sanglok3" />

    <ImageView
        android:id="@+id/imageDomitory"
        android:layout_width="156dp"
        android:layout_height="98dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="39dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageSanglok3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/domitory" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="98dp"
        android:text="@string/FirthFloor"
        app:layout_constraintBottom_toTopOf="@+id/imageSanglok3"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageSanglok1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="55dp"
        android:text="@string/SecondFloor"
        app:layout_constraintBottom_toTopOf="@+id/imageDomitory"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/imageSanglok2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="114dp"
        android:text="@string/ThirdFloor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageSanglok3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="105dp"
        android:text="@string/DormitoryRestaurant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/imageDomitory" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="309dp"
        android:layout_height="0dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="39dp"
        android:gravity="center"
        android:text="식사한 식당을 클릭해주세요"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageSanglok1"
        app:layout_constraintEnd_toStartOf="@+id/imageSanglok2"
        app:layout_constraintStart_toStartOf="@+id/imageSanglok2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageSanglok2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="33dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageSanglok1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sanglok2" />

</androidx.constraintlayout.widget.ConstraintLayout>